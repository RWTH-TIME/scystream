    {{task_id}} = DockerOperator(
            task_id='{{task_id}}',
            image='{{image}}',
            api_version='auto',
            auto_remove=True,
            command = "sh -c 'python -c \"from scystream.sdk.scheduler import Scheduler; \
            Scheduler.execute_function(\\\"{{ entry_name }}\\\")\"'"
            docker_url="unix://var/run/docker.sock",
            environment={{ environment }},
        )

